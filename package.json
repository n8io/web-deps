{
  "name": "web-deps",
  "description": "A dependency viewing app",
  "author": "Nate Clark <n8@n8io.com>",
  "version": "0.0.1",
  "private": false,
  "preferGlobal": false,
  "main": "src/server/app.js",
  "scripts": {
    "build": "npm run build:circleci && npm run build:nvm && npm run build:readme",
    "build:circleci": "node build/circleci/build",
    "build:docker": "node build/docker/build",
    "build:nvm": "node build/nvm/build",
    "build:readme": "node build/readme/build",
    "clean": "npm run clean:dusting && rm -rf node_modules/ src/client/statics/bower_components/ && npm run docker:clean",
    "clean:dusting": "rm -rf dist/ coverage/ npm-debug.log .git.json || true",
    "coverage": "gulp coverage",
    "dev": "gulp dev | bunyan",
    "docker": "npm run docker:prep && npm run docker:rebuild && npm run docker:up",
    "docker:build": "docker build -t n8io/web-deps:0.0.1 -f ./Dockerfile .",
    "docker:clean": "npm run docker:clean:containers && npm run docker:clean:image",
    "docker:clean:containers": "docker-compose stop && docker-compose rm -f",
    "docker:clean:image": "docker images -q n8io/web-deps:0.0.1 || docker images -q n8io/web-deps:0.0.1 | xargs docker rmi -f",
    "docker:prep": "gulp git-info && npm run build:docker",
    "docker:rebuild": "npm run docker:clean && npm run docker:build",
    "docker:restart": "npm run docker:clean:containers && npm run docker:prep && npm run docker:up",
    "docker:up": "docker-compose scale web=1 haproxy=1 mysql=1",
    "gulp": "gulp",
    "lint": "gulp lint",
    "postinstall": "gulp && npm run build:nvm",
    "start": "node $npm_package_main",
    "test": "gulp test"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "engines": {
    "node": "^5"
  },
  "bugs": {
    "url": "https://github.com/n8io/web-deps/issues"
  },
  "keywords": [
    "SumoMe",
    "survey",
    "NodeJs",
    "ExpressJs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/n8io/web-deps"
  },
  "readme": "README.md",
  "test": {
    "reporter": "spec"
  },
  "dependencies": {
    "babel-preset-es2015": "^6",
    "body-parser": "^1",
    "bower": "^1",
    "bunyan": "^1",
    "chalk": "^1",
    "cookie-parser": "^1",
    "cwd": "^0",
    "del": "^2",
    "dotenv-safe": "^2",
    "eslint-plugin-angular": "^0",
    "express": "^4",
    "express-stormpath": "^2",
    "glob-all": "^3",
    "gulp": "^3",
    "gulp-babel": "^6",
    "gulp-concat": "^2",
    "gulp-filter": "^3",
    "gulp-header": "^1",
    "gulp-if": "^2",
    "gulp-jsbeautifier": "^1",
    "gulp-livereload": "^3",
    "gulp-load-plugins": "^1",
    "gulp-ng-annotate": "^1",
    "gulp-rename": "^1",
    "gulp-sequence": "^0",
    "gulp-strip-debug": "^1",
    "gulp-stylus": "^2",
    "gulp-uglify": "^1",
    "gulp-util": "^3",
    "gulp-watch": "^4.3.5",
    "jade": "^1",
    "lodash": "^3",
    "moment": "^2",
    "morgan": "^1",
    "ng-annotate": "^1",
    "nib": "^1.1.0",
    "request": "^2",
    "semver": "^5",
    "serve-favicon": "^2",
    "serve-static": "^1",
    "shelljs": "^0"
  },
  "devDependencies": {
    "chai": "^3",
    "eslint": "^1",
    "ghooks": "^1",
    "gulp-debug": "^2",
    "gulp-eslint": "^1",
    "gulp-istanbul": "^0",
    "gulp-mocha": "^2",
    "gulp-nodemon": "^2",
    "gulp-reload": "0",
    "gulp-stylint": "^3",
    "istanbul": "^0",
    "json2yaml": "^1",
    "mocha": "^2",
    "node-dockerfile": "^0",
    "sort-json": "^1",
    "supertest": "^1"
  },
  "license": "MIT"
}
